  "terminal.integrated.allowChords": false,
  "explorer.autoReveal": true,
  "editor.fontSize": 15,
  "editor.fontWeight": 500,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.semanticHighlighting.enabled": false,
  // "editor.lineHeight": 30,
  "editor.mouseWheelZoom": true,
  "editor.rulers": [80, 120],
  // "editor.rulers": [80],
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSaveMode": "file",
  "editor.find.cursorMoveOnType": false,
  "editor.minimap.maxColumn": 160,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.scale": 1,
  // "editor.minimap.size": "proportional",
  "editor.wordWrapColumn": 160,
  "editor.wordWrap": "on",
  "editor.largeFileOptimizations": true,
  "editor.maxTokenizationLineLength": 9000,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": false,
    "source.fixAll.tslint": false,
    "source.fixAll.eslint": false,
    "eslint.autoFixOnSave": false
  },
  "editor.tabSize": 2,
  "editor.cursorWidth": 2,
  "editor.cursorBlinking": "smooth", // smooth | phase
  "editor.cursorStyle": "line",
  // "editor.renderWhitespace": "boundary", // all | boundary,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "smart", // on | off | smart
  "editor.detectIndentation": false,
  "editor.autoIndent": "none",
  // "editor.autoClosingQuotes": "always",
  // "editor.autoSurround": "quotes",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.guides.bracketPairs": true,
  // "editor.language.colorizedBracketPairs": [],
